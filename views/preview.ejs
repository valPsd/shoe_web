<script>
    $('#addCart').on('click', function () {
        const child = document.getElementById("product-svg");
        const form = $('#formCart');
        child.setAttribute(
            "style",
            "bottom: 2.8%; left: 0.3%"
        );
        html2canvas(document.querySelector("#container")).then(canvas => {
            canvas.toBlob(function (blob) {
                let fileName = 'shoeDoneModify.jpg'
                let file = new File([blob], fileName, { type: "image/jpeg", lastModified: new Date().getTime() }, 'utf-8');
                let container = new DataTransfer();
                container.items.add(file);
                document.querySelector('#shoeImg').files = container.files
                console.log(document.querySelector('#shoeImg').files);
                form.submit();
            });
        });
    })
</script>

