<!DOCTYPE html>
<html>

<head>
</head>

<body>
    <script>
        let msg = '<%- success %>';
        let path = '<%- path %>';
        if (path.indexOf(" ") >= 0 && path != " ") {
            let pathTemp = path.split(" ");
            path = pathTemp[0] + '/' + pathTemp[1]
        }
        if (msg != '') {
            console.log(path);
            if (!alert(msg)) { window.location.href = "/" + path }
        }
    </script>
</body>

</html>